<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.mcbain.scouting2018">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Manual" />
        <activity android:name=".TeamList" android:screenOrientation="sensorLandscape"/>
        <activity android:name=".ScoutSched" />
        <activity android:name=".MatchSched" />
        <activity android:name=".Contacts" />
        <activity android:name=".QRData" />
        <activity android:name=".License" />
        <activity android:name=".MenuScreen" />
        <activity android:name=".userinput"></activity>
    </application>

</manifest>